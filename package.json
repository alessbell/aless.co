{
  "name": "antipattern",
  "description": "alessia bellisario's blog",
  "version": "1.0.0",
  "engines": {
    "node": "12.x"
  },
  "author": "Alessia Bellisario <github@bellisar.io>",
  "repository": {
    "type": "git",
    "url": "https://github.com/alessbell/aless.co.git"
  },
  "dependencies": {
    "@next/mdx": "10.0.0",
    "date-fns": "2.16.1",
    "gl-react": "4.0.1",
    "gl-react-dom": "4.0.1",
    "gray-matter": "4.0.2",
    "next": "10.0.0",
    "nookies": "2.4.1",
    "query-string": "6.13.6",
    "raf": "3.4.1",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-flip-move": "3.0.4",
    "remark": "13.0.0",
    "remark-html": "13.0.1",
    "slugify": "1.4.5",
    "synthwave-vscode": "github:robb0wen/synthwave-vscode",
    "theme-bluloco-light": "github:uloco/theme-bluloco-light",
    "use-query-params": "1.1.8"
  },
  "keywords": [
    "nextjs",
    "typescript",
    "gl-react",
    "prettier"
  ],
  "license": "MIT",
  "main": "n/a",
  "scripts": {
    "build": "next build",
    "dev": "next dev",
    "start": "next start",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "type-check": "tsc --noEmit",
    "test": "npm run test:clear-cache && jest --coverage",
    "test:clear-cache": "rm -rf $(npx jest --showConfig | jq '.configs[0].cacheDirectory')",
    "lint": "eslint '{src,__tests__}/**/*.{js,jsx,ts,tsx}'"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run type-check && npm run lint"
    }
  },
  "devDependencies": {
    "@jamesacarr/jest-reporter-github-actions": "0.0.1",
    "@testing-library/dom": "7.26.3",
    "@testing-library/jest-dom": "5.11.5",
    "@testing-library/react": "11.1.0",
    "@testing-library/user-event": "12.1.10",
    "@types/gl-react": "3.15.3",
    "@types/gl-react-dom": "3.15.3",
    "@types/jest": "26.0.15",
    "@types/node": "14.14.3",
    "@types/raf": "3.4.0",
    "@types/react": "16.9.53",
    "@types/react-dom": "16.9.8",
    "@typescript-eslint/eslint-plugin": "4.5.0",
    "@typescript-eslint/parser": "4.5.0",
    "babel-jest": "26.6.1",
    "eslint": "7.12.0",
    "eslint-config-prettier": "6.14.0",
    "eslint-plugin-jest": "24.1.0",
    "eslint-plugin-jest-dom": "3.2.4",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-react": "7.21.5",
    "eslint-plugin-react-hooks": "4.2.0",
    "eslint-plugin-testing-library": "3.9.2",
    "husky": "4.3.0",
    "identity-obj-proxy": "3.0.0",
    "jest": "26.6.1",
    "jest-canvas-mock": "2.3.0",
    "prettier": "2.1.2",
    "ts-jest": "26.4.2",
    "typescript": "4.0.3"
  }
}
